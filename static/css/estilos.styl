						font-family 'iconos'
						content '\e60d'
						font-size 40px

				



		#resultadosJugador
			width 100%
			font-family: 'Hammersmith One', sans-serif;
			font-size 20px
			border 1px solid black
			background-color: rgba(0, 0, 0, 0.1);
			box-shadow 5px 5px 5px 5px rgba(0, 0, 0, 0.4);
			.enviarbtn
				width 50%
				font-size 20px
				position relative
				left 80px
			

			#fecha
				width 100%
				background #333 
				font-family: 'Hammersmith One', sans-serif;
				h2
					left 250px
					margin 0
					color white
				#inicio
					color white
					font-size 16px
			.alertForm
				width 100%
				position relative
				top 0px
				strong
					position relative
			#equipos
				
				.resultado
					
					input
						height 31px
						width 50px
		
		.tablaFecha
				background-color: rgba(0, 0, 0, 0.1);
				width 100%
				position relative
				top 20px
				box-shadow 5px 5px 5px 5px rgba(0, 0, 0, 0.4);
				


				h2
					font-size 20px
					margin 0
					background #333
					color white
				table
					position relative
					left 100px
					top 0px
					width 50%

		.a_demo_two
			 
			    background-color #6fba26
			    padding 10px
			    position relative
			    font-family  'Open Sans', sans-serif
			    font-size 12px
			    text-decoration none
			    color #fff
			    background-image  linear-gradient(bottom, rgb(100,170,30) 0%, rgb(129,212,51) 100%)
			    box-shadow  inset 0px 1px 0px #b2f17f, 0px 6px 0px #3d6f0d
			    border-radius  5px
		
	 
		.a_demo_two:active 
			
			    top 7px
			    background-image  linear-gradient(bottom, rgb(100,170,30) 100%, rgb(129,212,51) 0%)
			    box-shadow  inset 0px 1px 0px #b2f17f, inset 0px -1px 0px #3d6f0d
			    color  #156785
			    text-shadow  0px 1px 1px rgba(255,255,255,0.3)
			    background  rgb(44,160,202)

		.enviar
			position relative
			top 10px
		.borrar
			background-color #e73438
			box-shadow  inset 0px 1px 0px #b2f17f, 0px 6px 0px #a01317
			position relative
			left 10px
			top 10px

		

