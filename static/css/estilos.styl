@font-face {
	font-family: 'iconos';
	src:url('fonts/icomoon.eot');
	src:url('fonts/icomoon.eot?#iefix') format('embedded-opentype'),
		url('fonts/icomoon.woff') format('woff'),
		url('fonts/icomoon.ttf') format('truetype'),
		url('fonts/icomoon.svg#icomoon') format('svg');
	font-weight: normal;
	font-style: normal;
}
body
	background #FFF
	

	#pasosModalLabel
		background #333
		color white
		text-align center
		padding 0.5em
	#grupo
		padding 0.8em 2em
		width 200px
		font-size 12px
		&:after
			font-family 'iconos'
			position relative
			left 10px
			content '\e606'
	#grupoModal
		padding 0.5em 1em
		width 200px
		font-size 12px
		&:after
			font-family 'iconos'
			position relative
			left 5px
			content '\e606'
				
	#twitter
		padding 0.8em 2em
		font-size 12px
		&:after
			font-family 'iconos'
			position relative
			left 10px
			content '\e604'
			
	.registro
		position absolute
		top 400px
		left 0px
